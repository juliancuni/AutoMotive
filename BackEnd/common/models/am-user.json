{
  "name": "amUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "restrictResetPasswordTokenScope": true,
  "emailVerificationRequired": true,
  "properties": {
    "emer": {
      "type": "string",
      "required": true
    },
    "mbiemer": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "telefon": {
      "type": "string"
    },
    "datelindja": {
      "type": "date"
    },
    "avatar": {
      "type": "string",
      "required": true,
      "default": "assets/img/no-pic.png"
    },
    "enabled": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "adresa": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
